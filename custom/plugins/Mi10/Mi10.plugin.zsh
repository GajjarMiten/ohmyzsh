# Flutter commands
alias fl="flutter"
alias flr="flutter run"
alias fldoc="flutter doctor"
alias flb="flutter build"
alias flbw="flutter build web"
alias flattach="flutter attach"
alias flget="flutter packages get"
alias flc="flutter clean"
alias flcr="flutter create"
alias flrp="flutter run --profile"
alias flrr="flutter run --release"

# VS Code commands
alias vsc="code ." 
alias vs="code"

# To edit .zshrc
alias zshrc="code ~/.zshrc"

# To install pkgs
alias sapt="sudo apt install"
alias saptg="sudo apt-get install"

# To clear terminal Screen
alias cls="clear"

alias cd..="cd -"